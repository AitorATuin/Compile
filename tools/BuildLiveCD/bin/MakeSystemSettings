#!/bin/sh

cat <<EOF > GoboLinuxVersion
013-pre1
EOF

cat <<EOF > gobo
LFS 3521
2002-03-20 01:51
EOF

cat <<EOF > group
gobo:x:0:
bin:x:1:
sys:x:2:gobo
kmem:x:3:
tty:x:4:
tape:x:5:
daemon:x:6:
floppy:x:7:
disk:x:8:
lp:x:9:
dialout:x:10:
audio:x:11:
nobody:x:12:
users:x:100:
uucp:x:15
ftp:x:101:
fibo:x:21:
cdrecord:x:102:
video:x:103:
sshd:x:104:
EOF

cat <<EOF > hosts
127.0.0.1 localhost.localdomain localhost LiveCD
EOF

cat <<EOF > passwd
gobo:x:0:0:SuperUser:/Users/gobo:/bin/zsh
nobody:x:12:12:nobody:/tmp:
mail:x:8:12:mail:/:
news:x:9:13:news:/usr/lib/news:
uucp:x:15:15:uucp:/var/spool/uucp:
ftp:x:14:50::/var/ftp/tmp:
fibo:x:21:21::/tmp:/bin/zsh
sshd:x:22:104::/var/empty:/bin/false
EOF

cat <<EOF > resolv.conf
#nameserver 200.176.2.10
#nameserver 200.177.250.10
EOF

cat <<EOF > shadow
gobo:aSt9IU/wQKWM2:13184::::::
nobody:*:9797:0:::::
uucp:*:9797:0:::::
ftp:!:12105:0:99999:7:::
fibo:!:12154:0:99999:7:::
sshd:!:12341:0:99999:7:::
EOF

cat <<EOF > fstab
# Sys filesystem
none /System/Kernel/Objects sysfs defaults 0 0

# Proc filesystem.
proc /System/Kernel/Status proc defaults 0 0

# USB filesystem.
usbfs /System/Kernel/Status/bus/usb usbfs defaults 0 0

# /dev/shm (shared memory)
none /System/Kernel/Devices/shm tmpfs defaults 0 0

# Pseudo-TTYs
none /System/Kernel/Devices/pts devpts defaults 0 0

# Floppy drive.
/System/Kernel/Devices/floppy/0 /Mount/Floppy auto noauto,users,rw 1 0
EOF
